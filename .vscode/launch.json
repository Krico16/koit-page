    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Gatsby develop",
            "type": "pwa-node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/.bin/gatsby",
            "windows": {
                "program": "${workspaceFolder}/node_modules/gatsby/dist/bin/gatsby"
            },
            "args": ["develop"],
            "runtimeArgs": ["--nolazy"],
            "console": "integratedTerminal"
        }
    ]
}